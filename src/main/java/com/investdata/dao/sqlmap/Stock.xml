<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="stock">
	
	<!-- 在此处用typeAlias标签声明后，后面的标签可以直接使用user代替 包名 + 类名的写法 -->
	<typeAlias alias="stock" type="com.investdata.dao.po.Stock"/>
	
	
	<select id="getStocks" parameterClass="com.investdata.dao.po.Stock" resultClass="stock">
		select code,name,shortname,market from t_stocks
	</select>
